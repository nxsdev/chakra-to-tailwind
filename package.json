{
  "name": "chakra-to-tailwind",
  "version": "0.0.0",
  "description": "Converts Chakra UI components to Tailwind CSS",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "lint": "biome lint .",
    "format": "biome format . --write"
  },
  "keywords": ["chakra-ui", "tailwindcss", "converter"],
  "author": "nxsdev",
  "license": "MIT",
  "dependencies": {
    "@chakra-ui/react": "^2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.10.5",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "packageManager": "pnpm@9.15.2",
  "volta": {
    "node": "22.12.0",
    "pnpm": "9.15.2"
  }
}
